{
  "name": "pt2itp",
  "version": "21.0.1",
  "license": "BSD-2-Clause ",
  "description": "Attach interpolation values given a road network and address points",
  "main": "index.js",
  "bin": {
    "pt2itp": "./index.js"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "build": "neon build --release",
    "lint": "eslint index.js test/ lib/",
    "package": "neon build --release && node-pre-gyp package && node-pre-gyp-github publish",
    "coverage": "nyc tape test/*.js && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage-upload": "coveralls < ./coverage.lcov",
    "doc": "./node_modules/documentation/bin/documentation.js lint ./lib/**/*.js",
    "test": "tape test/*.test.js && cd native && cargo test --release"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/ingalls/pt2itp/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native/",
    "pkg_path": "."
  },
  "author": "Nicholas Ingalls <nicholas.ingalls@gmail.com>",
  "dependencies": {
    "@mapbox/carmen": "27.2.0",
    "@mapbox/eslint-config-geocoding": "^2.0.0",
    "@mapbox/geocoder-abbreviations": "2.4.1",
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/mbtiles": "^0.10.0",
    "@mapbox/tile-cover": "^3.0.2",
    "@mapbox/tilebelt": "^1.0.1",
    "@mapbox/title-case": "^1.3.0",
    "@turf/turf": "^5.1.0",
    "csv-stringify": "^5.0.0",
    "d3-queue": "^3.0.7",
    "diacritics": "^1.3.0",
    "eslint-plugin-node": "^8.0.1",
    "express": "^4.15.3",
    "fast-csv": "^2.4.0",
    "fast-levenshtein": "^2.0.6",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "n-readlines": "^1.0.0",
    "neon-cli": "^0.2.0",
    "node-pre-gyp": "~0.12.0",
    "node-pre-gyp-github": "^1.4.3",
    "pg": "^7.3.0",
    "pg-copy-streams": "^2.1.0",
    "pg-cursor": "^2.0.0",
    "pluralize": "^7.0.0",
    "progress": "^2.0.0",
    "simple-statistics": "^7.0.0",
    "split": "^1.0.0",
    "talisman": "^0.21.0",
    "turf-line-slice-at-intersection": "^1.0.1",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "documentation": "^10.1.0",
    "eslint": "^5.3.0",
    "nyc": "^14.0.0",
    "tape": "^4.7.0",
    "tape-spawn": "^1.4.2",
    "tmp": "0.1.0"
  },
  "eslintConfig": {
    "extends": "@mapbox/eslint-config-geocoding"
  },
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingalls/pt2itp.git"
  }
}
